{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "plugins": [
        "react",
        "babel"
    ],
    "rules": {
        "max-len": ["error", { "code": 160 }],
        "no-unused-vars": ["error", "argsIgnorePattern": "^_"],
        "no-console": 0,
    }
}
